// normal identifiers
Ident: Ident = {
    <l: @L> <i: "ident"> <r: @R> => {
        let sym = match i {
            Token::Ident(ident) => ident,
            _ => unreachable!(),
        };
        Ident {
            sym,
            span: Span{ start: l, end: r },
        }
    },
};


// Yield - if non-empty, exclude yield.
// Await - if non-empty, exclude await.
IdentRef<Yield, Await>: Ident = {
    Ident,
    "yield" if Yield == "",
    "await" if Await == "",
};


BindingIdent<Yield, Await>: Ident = {
    Ident,
    "yield",
    "await",    
};

// Yield - if non-empty, exclude yield.
// Await - if non-empty, exclude await.
LabelIdent<Yield, Await>: Ident = {
    Ident,
    "yield" if Yield == "",
    "await" if Await == "",
};




