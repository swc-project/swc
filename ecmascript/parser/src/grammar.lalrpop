// -------------------- Macros --------------------

// TODO: Use a trait like WithSpan

SpannedExpr<Rule>: Expr = 
    <l: @L> <node: Rule> <r: @R> => {
        Expr {
            span: Span { start: l, end: r },
            node,
        }
    };


SingleComma: () = {
    "," => (),
    // Necessar
    <!> => errors.push(<>.error)
};

Comma<Rule>: Vec<Rule> =
    <rules: (<Rule> SingleComma)*> <last: Rule?> => {
        let mut rules = rules;
        rules.extend(last);
        rules
    };


