{
  "name": "@swc/core",
  "version": "1.2.23",
  "description": "Super-fast alternative for babel",
  "homepage": "https://swc-project.github.io",
  "main": "./index.js",
  "author": "강동윤 <kdy1997.dev@gmail.com>",
  "license": "MIT",
  "keywords": [
