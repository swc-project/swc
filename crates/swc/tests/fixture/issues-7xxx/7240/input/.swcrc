{
    "jsc": {
        "parser": {
            "syntax": "ecmascript",
            "jsx": false
        },
        "target": "es5",
        "loose": true,
        "minify": {
            "compress": {
                "dead_code": true,
                "drop_console": true,
                "drop_debugger": true,
                "passes": 3
            },
            "format": {
                "asciiOnly": true
            },
            "mangle": {
                "toplevel": false,
                "keep_classnames": false,
                "keep_fnames": false,
                "keep_private_props": false,
                "ie8": false,
                "safari10": false
            }
        }
    },
    "module": {
        "type": "commonjs"
    },
    "isModule": true
}