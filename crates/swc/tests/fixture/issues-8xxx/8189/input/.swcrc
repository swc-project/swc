{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true
        },
        "target": "es2020",
        "loose": false,
        "minify": {
            "compress": false,
            "mangle": false,
            "format": {
                "asciiOnly": true
            }
        },
        "transform": {
            "react": {
                "runtime": "automatic"
            }
        }
    },
    "module": {
        "type": "es6"
    },
    "minify": true,
    "isModule": true
}