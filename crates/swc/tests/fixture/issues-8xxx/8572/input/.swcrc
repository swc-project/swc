{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true
        },
        "target": "es2022",
        "loose": false,
        "minify": {
            "compress": false,
            "mangle": false
        },
        "transform": {
            "react": {
                "importSource": "npm:react",
                "runtime": "automatic"
            }
        }
    },
    "module": {
        "type": "es6"
    },
    "minify": false,
    "isModule": true
}