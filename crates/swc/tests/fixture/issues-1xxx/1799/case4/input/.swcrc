{
    "jsc": {
        "transform": {
            "hidden": {
                "jest": true
            },
            "react": {
                "runtime": "classic"
            }
        },
        "parser": {
            "syntax": "ecmascript",
            "jsx": true,
            "dynamicImport": true,
            "privateMethod": false,
            "functionBind": false,
            "exportDefaultFrom": true,
            "exportNamespaceFrom": true,
            "decorators": false,
            "decoratorsBeforeExport": false,
            "topLevelAwait": false,
            "importMeta": false
        },
        "target": "es2021",
        "loose": true,
        "externalHelpers": true,
        "keepClassNames": true
    },
    "module": {
        "type": "commonjs"
    }
}