{
    "sourceMaps": false,
    "module": {
        "type": "commonjs"
    },
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "dynamicImport": true,
            "decorators": false
        },
        "transform": {
            "react": {
                "pragma": "React.createElement",
                "pragmaFrag": "React.Fragment",
                "throwIfNamespace": true,
                "development": false,
                "useBuiltins": false
            },
            "hidden": {
                "jest": true
            }
        },
        "target": "es2015",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": false,
        "preserveAllComments": true
    },
    "minify": false
}
