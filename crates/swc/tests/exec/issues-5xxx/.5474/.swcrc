{
    "jsc": {
        "target": "es5",
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": false,
            "decoratorMetadata": true
        }
    },
    "sourceMaps": true,
    "module": {
        "type": "commonjs"
    }
}